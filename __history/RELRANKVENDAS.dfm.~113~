  object QRYRKVEND: TUniQuery
    Connection = DataModule1.ConDados
    SQL.Strings = (
      'SELECT'
      '    P.Produto,'
      '    P.CodInterno,'
      
        '    SUM(CAST(REPLACE(ISNULL(I.Total, '#39'0'#39'), '#39','#39', '#39'.'#39') AS DECIMAL(' +
        '18, 2)) - CAST(REPLACE(ISNULL(I.DescRateio, '#39'0'#39'), '#39','#39', '#39'.'#39') AS D' +
        'ECIMAL(18, 2)) + CAST(REPLACE(ISNULL(I.AcrescRateio, '#39'0'#39'), '#39','#39', ' +
        #39'.'#39') AS DECIMAL(18, 2))) AS ValorTotal,'
      '    SUM(I.QtdReal) AS QuantidadeVendida,'
      '    CAST(:Dias AS VARCHAR(10)) AS Periodo,'
      '    P.Unidade,'
      '    P.Quantidade AS Estoque,'
      '    NE.CNPJ,'
      '    NE.Razao_Social,'
      '    G.Setor AS Grupo,'
      '    F.Empresa AS Fornecedor,'
      '    P.Fabricante AS Marca,'
      '    P.naosaitabela,'
      '    P.MOEDA, -- O novo campo que voc'#234' pediu'
      '    ('
      '        SELECT'
      
        '            SUM(CAST(REPLACE(ISNULL(T.Total, '#39'0'#39'), '#39','#39', '#39'.'#39') AS ' +
        'DECIMAL(18, 2)) - CAST(REPLACE(ISNULL(T.DescRateio, '#39'0'#39'), '#39','#39', '#39 +
        '.'#39') AS DECIMAL(18, 2)) + CAST(REPLACE(ISNULL(T.AcrescRateio, '#39'0'#39 +
        '), '#39','#39', '#39'.'#39') AS DECIMAL(18, 2)))'
      '        FROM'
      '            tabest3b T WITH (NOLOCK)'
      
        '        LEFT JOIN tabest3a V2 WITH (NOLOCK) ON T.pedido = V2.ped' +
        'ido'
      '        WHERE'
      
        '            T.DataInc >= DATEADD(DAY, -:Dias, CAST(GETDATE() AS ' +
        'DATE))'
      
        '            AND CAST(REPLACE(ISNULL(T.Total, '#39'0'#39'), '#39','#39', '#39'.'#39') AS ' +
        'DECIMAL(18, 2)) > 0'
      '            AND V2.Cancelada <> 1'
      '            AND V2.Venda = 1'
      '            AND V2.Status = '#39'V'#39
      '    ) AS ValorTotalPeriodo'
      'FROM'
      '    tabest3b I WITH (NOLOCK)'
      'LEFT JOIN tabest1 P WITH (NOLOCK) ON I.lkProduto = P.controle'
      'LEFT JOIN tabest3a V WITH (NOLOCK) ON I.pedido = V.pedido'
      'LEFT JOIN tabest8 G WITH (NOLOCK) ON P.lkSetor = G.Controle'
      'LEFT JOIN tabfor F WITH (NOLOCK) ON P.lkfornec = F.Controle'
      'LEFT JOIN nfe_emitente NE ON I.idempresa = NE.codigo'
      'WHERE'
      
        '    CAST(REPLACE(ISNULL(I.Total, '#39'0'#39'), '#39','#39', '#39'.'#39') AS DECIMAL(18, ' +
        '2)) > 0'
      '    AND V.Cancelada <> 1'
      '    AND V.Venda = 1'
      '    AND V.Status = '#39'V'#39
      
        '    AND I.DataInc >= DATEADD(DAY, -:Dias, CAST(GETDATE() AS DATE' +
        '))'
      'GROUP BY'
      '    P.Produto,'
      '    P.CodInterno,'
      '    P.Unidade,'
      '    P.Quantidade,'
      '    NE.CNPJ,'
      '    NE.Razao_Social,'
      '    G.Setor,'
      '    F.Empresa,'
      '    P.Fabricante,'
      '    P.naosaitabela,'
      '    P.MOEDA -- O novo campo precisa ser adicionado aqui tamb'#233'm'
      'ORDER BY'
      '    SUM(I.QtdReal) DESC;')
    Left = 288
    Top = 40
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'Dias'
        Value = Null
      end>
  end
  object QRYMARCA: TUniQuery
    Connection = DataModule1.ConDados
    SQL.Strings = (
      'SELECT DISTINCT'
      '    FABRICANTE'
      'FROM'
      '    tabest1'
      'WHERE'
      '    FABRICANTE IS NOT NULL'
      '    AND FABRICANTE <> '#39#39
      'ORDER BY'
      '    FABRICANTE;')
    Left = 257
    Top = 33
  end
